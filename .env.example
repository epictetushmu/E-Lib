
# CAS Authentication
CAS_SERVER_URL=https://your-cas-server.com/cas

# MongoDB Connection Options
# Option 1: Connection URI
MONGO_URI=mongodb://mongo:27017/myappdb

# Option 2: Connection Parameters
MONGODB_HOST=mongo
MONGODB_PORT=27017
MONGODB_USERNAME=root
MONGODB_PASSWORD=example
MONGODB_DATABASE=e_lib
MONGODB_AUTH_SOURCE=admin

# Application Settings
APP_ENV=development
APP_DEBUG=true
APP_URL=http://localhost:8080

# Ngrok Configuration (for development tunneling)
NGROK_AUTH_TOKEN=your_ngrok_auth_token_here
